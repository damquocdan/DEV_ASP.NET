@model X.PagedList.IPagedList<Lesson09.Lap.Models.AdminLog>
@using X.PagedList.Mvc.Core;

@{
    ViewData["Title"] = "AdminLog";
    Layout = "~/Areas/Admins/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>AdminLog</h1>
<div class="row">
    <form action="" method="get" class="form-inline">
        <div class="form-group">
            <label class="sr-only" for="keyword">Tìm kiếm</label>
            <input class="form-control" name="name" id="keyword" placeholder="Nhập tên cần tìm" />
        </div>
        <button type="submit" class="btn btn-dèault mr-1"><i class="fas fa-search"></i></button>
        <a asp-action="Create" class="btn btn-primary"><i class="fas fa-plus"></i>Thêm mới</a>
    </form>
</div>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstOrDefault().Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstOrDefault().SessionId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstOrDefault().AppCode)
